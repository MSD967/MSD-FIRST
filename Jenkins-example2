pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'checkout'
            }
        }
        stage('Test') {
            steps {
                echo 'Test env'
            }
        }
        stage('build') {
            steps {
                echo 'build code'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy code'
            }
        }
    }
}
